<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<manifest>
    <application name="Test">
        <crown name="Test">
            <desc>user script</desc>
            <serves>
                <function name="addProfile">
                    <trait>released</trait>
                    <param desc="" multiplicity="1" name="partNumber" type="string"/>
                    <param desc="" multiplicity="1" name="serialNumber" type="string"/>
                    <param desc="MQTT / HTTP" multiplicity="1" name="connectionType" type="string"/>
                    <param desc="" multiplicity="1" name="profileName" type="string"/>
                    <return desc="" multiplicity="1" name="data" type="string"/>
                </function>
                <function name="deleteAllProfiles">
                    <trait>released</trait>
                    <return desc="" multiplicity="1" name="status" type="string"/>
                </function>
                <function name="getProfiles">
                    <trait>released</trait>
                    <return desc="" multiplicity="1" name="data" type="string"/>
                </function>
            </serves>
        </crown>
        <meta key="author">Andreas Verdenhalven</meta>
        <meta key="version">1.0</meta>
        <meta key="priority">low</meta>
        <meta key="copy-protected">false</meta>
        <meta key="read-protected">false</meta>
        <entry default="Test.lua" path="scripts"/>
    </application>
</manifest>
